<div class="wrapper">
    <div class="container">
      <div class="row">
        <h1>Evaluation Rubric</h1>
        <div class="col-sm-12">
          <!-- Create New Button -->
          <button class="btn btn-success" type="button" name="button"><a id="myCreateBtn" href="{{create}}">Create New</a></button>
          <!-- Table -->
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Name</th>
                <th scope="col">Evaluation Rubric ID</th>
                <th scope="col">Description</th>
                <th scope="col">Student Outcome ID</th>
                <th scope="col">Date Created</th>
                <th scope="col"> </th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>{{name}}</td>
                <td>{{rubricID}}</td>
                <td>{{description}}</td>
                <td>{{studOutID}}</td>
                <td>{{dateCreated}}</td>
                <td>
                  <button class="btn btn-danger" type="button" name="button"><a id="editBtn" href="{{edit}}">Edit</a></button>
                  <button class="btn btn-warning" type="button" name="button"><a id="deleteBtn" href="{{delete}}">Delete</a></button>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

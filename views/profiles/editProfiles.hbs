<div class="wrapper">
  <div class="container">
    <div class="row">
      <h1>Evaluation Rubric</h1>
      <div class="col-sm-12">
        <form>
          <div class="row">
            <div class="col-sm-2">
              <label>Name</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="text" placeholder="Rubric Name">
            </div>
          </div>
          <div class="row">
            <div class="col-sm-2">
              <label>Description</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="text" placeholder="Rubric Description">
            </div>
          </div>
          <div class="row">
            <div class="col-sm-2">
              <label>Student Outcome</label>
            </div>
            <div class="col-sm-10">
              <select class="form-control">
                <option value="0"> Student Outcome Name </option>
              </select>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-2">
              <label>Date Created</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="text" placeholder="Date Created">
            </div>
          </div>
        </form>
      </div>
        <button id="createModalbtn" type="button" class="btn">Save</button>
      </div>
    </div>
  </div>
</div>
